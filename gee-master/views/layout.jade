doctype html
html
	head
		if title
			title PI - #{title}
		else
			title PlanetIgnite
		link(href='/styles/main.css' rel='stylesheet' type='text/css')
		link(href='http://fonts.googleapis.com/css?family=PT+Sans' rel='stylesheet' type='text/css')
		block custHead
	body
		div#imageHeader
			a(href='/')
				img(src='/images/PlanetIgniteLogo.png' alt='PlanetIgnite Logo') 
		div#fullWrapper
			div#pageHeader
				block pageHeader
			div#pageOptions
				block pageOptions
			br
			div#content
				block content
		div#footer <a href='/feedback'>Feedback</a> | <a href='http://geni.net' target='_blank'>GENI</a> | <a href='/logout/'>Logout</a>
