doctype html
html
head 
  title User List
body
  center 
    h1 GEE Users
    table(border=1)
      tr
        th User
        th Admin
        th Slice
      
      each user, i in userlist
        tr
          td #{user.email}
          td #{user.admin}
          td #{user.slice}
    br
    br
    form(method="get", action="/add_user")
      table
        tr
          td New User
          td: input(type="text", name="user", size=30)
        tr
          td(colspan=2, style="text-align:center")
            button(type="submit")  Add User